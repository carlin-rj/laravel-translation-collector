{
    "name": "carlin/laravel-translation-collector",
    "description": "Laravel Translation Collector Package - 自动收集项目中的翻译文本并同步到外部翻译系统",
    "type": "library",
    "keywords": ["laravel", "translation", "i18n", "localization", "collector"],
    "license": "MIT",
    "authors": [
        {
            "name": "YC Development Team",
            "email": "dev@yc.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.75|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.75|^9.0|^10.0|^11.0",
        "illuminate/console": "^8.75|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/finder": "^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Carlin\\LaravelTranslationCollector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carlin\\LaravelTranslationCollector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carlin\\LaravelTranslationCollector\\TranslationCollectorServiceProvider"
            ],
            "aliases": {
                "TranslationCollector": "Carlin\\LaravelTranslationCollector\\Facades\\TranslationCollector"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html=coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
